{
	"name": "belsignum/disposable-email",
	"type": "typo3-cms-extension",
	"description": "A service extension to compile lists of disposable and free mail providers, offering a simple way to validate email addresses against them.",
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "Andreas Sommer",
			"role": "Developer"
		}
	],
	"keywords": [
		"TYPO3 CMS",
		"TYPO3 extension",
		"Disposable Email provider list"
	],
	"require": {
	},
	"require-dev": {
		"phpunit/phpunit": "^10.5",
		"typo3/testing-framework": "^8.0"
	},
	"autoload": {
		"psr-4": {
			"Belsignum\\DisposableEmail\\": "Classes/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Belsignum\\DisposableEmail\\Tests\\": "Tests/"
		}
	},
	"scripts": {
		"test:unit": "phpunit -c Build/phpunit/UnitTests.xml",
		"test:functional": "phpunit -c Build/phpunit/FunctionalTests.xml"
	},
	"config": {
		"allow-plugins": {
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "disposable_email"
		}
	}
}
